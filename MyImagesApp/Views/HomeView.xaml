<UserControl x:Class="MyImagesApp.Views.HomeView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MyImagesApp.Views"
             xmlns:vml="clr-namespace:MyImagesApp.VML"
             vml:ViewModelLocator.AutoHookedUpViewModel="True"
             mc:Ignorable="d" 
             d:DesignHeight="534" d:DesignWidth="670">
    <Grid Background="Transparent">
        <ScrollViewer VerticalScrollBarVisibility="Auto">
            <StackPanel Name="spHome" HorizontalAlignment="Left" Margin="140,0,0,0" VerticalAlignment="Top" Width="390" Background="WhiteSmoke">
            </StackPanel>
        </ScrollViewer>
    </Grid>
</UserControl>
